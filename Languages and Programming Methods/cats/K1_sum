#include <iostream>
#include <math.h>
#include <bitset>
#include <string>
#include <fstream>

using namespace  std;

string bit_num (int num, int n)
{
    string result = "";
    while (num)
    {
        result = to_string(num % 2) + result;
        num /= 2;
    }
    while (result.length() < n)
    {
        result = "0" + result;
    }
    return result;
}
void PrintAll (int n, ofstream &out)
{
    int max = pow (2, n);
    for (int i = 0; i < max; i++)
    {
        out << bit_num(i, n) << "\n";
    }
}

int main()
{
    ifstream in ("input.txt");
    ofstream out ("output.txt");

    int n;
    in >> n;
    PrintAll(n, out);
    in.close();
    out.close();
    return 0;
}
